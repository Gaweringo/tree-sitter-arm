{
  "name": "asm",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "function_definition"
        }
      ]
    },
    "function_definition": {
      "type": "PATTERN",
      "value": "[[a-z]+][:]"
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "return_statement"
        }
      ]
    },
    "add_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "add"
        },
        {
          "type": "SYMBOL",
          "name": "register"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "register"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "register"
        }
      ]
    },
    "return_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "return_register"
        },
        {
          "type": "SYMBOL",
          "name": "register"
        }
      ]
    },
    "return_register": {
      "type": "PATTERN",
      "value": "(bx)\\s+"
    },
    "special_register": {
      "type": "STRING",
      "value": "lr"
    },
    "register": {
      "type": "PATTERN",
      "value": "[r]\\d+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

